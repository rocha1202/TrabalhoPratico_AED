# configurações padrões do programa
# os comentarios começam com um jogo da velha (#)
# seguem uma estrutura de key=value
# recomendado não conter espaços entre eles

# nome do aplicativo
app_name="GT5 - Gestor de Jogos"    

# usuario admin padrão ao instalar o programa pela primeira vez
default_admin="admin"
default_pass="1234"

# a palleta de cores a usar
# tem que ser um caminho relativo ao desta pasta
theme=".config/theme.config"